#!/usr/bin/env bash

source project.conf

rustc -c -o output/lib/o/vec.o source/rust/common/vec.rs
rustc -o output/lib/so/vec source/rust/common/vec.rs

rustc -o output/lib/so/dynamics -L output/lib/so source/rust/common/dynamics.rs

./scripts/zwobot/zwobot-compile \
	"$CORE_SERVICE_SOURCE" "$OUTPUT_LIB/o/vec.o" "" $CORE_SERVICE_BINARY
