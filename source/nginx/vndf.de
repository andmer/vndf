server {
	listen	80;
	listen	[::]:80;

	server_name	www.vndf.de;

	access_log	/var/log/nginx/vndf.de.access.log;

	location / {
		root	/srv/http/vndf.de;
		index	index.html index.htm;
	}
}

server {
	listen	80;
	listen	[::]:80;

	server_name	vndf.de;

	return	301 $scheme://www.$server_name$request_uri;
}
